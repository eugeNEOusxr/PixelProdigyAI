  "screenshots": [
    {
      "src": "/screenshots/geneclone-home.png",
      "sizes": "1080x1920",
      "type": "image/png",
      "label": "Gene Clone Phone home screen with instant app access"
    },
    {
      "src": "/screenshots/geneclone-apps.png",
      "sizes": "1080x1920",
      "type": "image/png",
      "label": "10,000+ apps available without downloads"
    }
  ],
  "shortcuts": [
    {
      "name": "PixelProdigy 3D",
      "short_name": "3D Studio",
      "description": "Open 3D creation studio",
      "url": "/pixelprodigy3d.html",
      "icons": [{ "src": "/icons/pixelprodigy-icon.png", "sizes": "96x96" }]
    },
    {
      "name": "WordWeaver",
      "short_name": "Documents",
      "description": "Open document editor",
      "url": "/pixelprodigy3d.html?workspace=wordweaver",
      "icons": [{ "src": "/icons/wordweaver-icon.png", "sizes": "96x96" }]
    },
    {
      "name": "Analytics",
      "short_name": "Analytics",
      "description": "View live analytics",
      "url": "/live_analytics_dashboard.html",
      "icons": [{ "src": "/icons/analytics-icon.png", "sizes": "96x96" }]
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "media",
          "accept": ["image/*", "video/*", "audio/*", ".pdf", ".docx", ".obj", ".fbx", ".glb"]
        }
      ]
    }
  },
  "protocol_handlers": [
    {
      "protocol": "web+eugeneous",
      "url": "/geneclone_phone.html?action=%s"
    }
  ],
  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=com.eugeneous.geneclone",
      "id": "com.eugeneous.geneclone"
    }
  ],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  "file_handlers": [
    {
      "action": "/open-file",
      "accept": {
        "application/3d-model": [".obj", ".fbx", ".glb", ".gltf"],
        "application/pixelprodigy": [".pp3d", ".ppw"],
        "image/*": [".png", ".jpg", ".jpeg", ".svg", ".webp"]
      }
    }
  ]
}
